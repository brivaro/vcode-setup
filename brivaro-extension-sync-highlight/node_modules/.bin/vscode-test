else
  export NODE_PATH="/mnt/c/Users/brian/Documents/Videos y contenido del canal/AGENDA/7 - IA/DocSapiens RAG/DocSapiens/brivaro-extension-sync-highlight/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/out/node_modules:/mnt/c/Users/brian/Documents/Videos y contenido del canal/AGENDA/7 - IA/DocSapiens RAG/DocSapiens/brivaro-extension-sync-highlight/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/test-cli/node_modules:/mnt/c/Users/brian/Documents/Videos y contenido del canal/AGENDA/7 - IA/DocSapiens RAG/DocSapiens/brivaro-extension-sync-highlight/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules/@vscode/node_modules:/mnt/c/Users/brian/Documents/Videos y contenido del canal/AGENDA/7 - IA/DocSapiens RAG/DocSapiens/brivaro-extension-sync-highlight/node_modules/.pnpm/@vscode+test-cli@0.0.12/node_modules:/mnt/c/Users/brian/Documents/Videos y contenido del canal/AGENDA/7 - IA/DocSapiens RAG/DocSapiens/brivaro-extension-sync-highlight/node_modules/.pnpm/node_modules:$NODE_PATH"
